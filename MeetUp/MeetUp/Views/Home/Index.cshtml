
@{
    Layout = null;
    if(Session["EmployeeId"] == null)
    {
        Response.Redirect("~/Login/Index");
    }
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div> 
        <h1>Dashboard</h1>
        <h3>Welcome : @Session["EmployeeId"].ToString()</h3>
        <a href="@Url.Action("LogOut","Login")">Logout</a>"
    </div>
</body>
</html>
